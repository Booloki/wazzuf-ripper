<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE Tags SYSTEM "matroskatags.dtd">
<Tags>
    <Tag> <!-- show -->
        <Targets>
            <TargetTypeValue>70</TargetTypeValue>
        </Targets>
        <Simple>
            <Name>TITLE</Name>
            <String>XMLTAG_SHOW</String>
        </Simple>
    </Tag>
    <Tag> <!-- season -->
        <Targets>
            <TargetTypeValue>60</TargetTypeValue>
        </Targets>
        <Simple>
            <Name>PART_NUMBER</Name>
            <String>XMLTAG_SEASON</String> <!-- season number -->
        </Simple>
        <Simple>
            <Name>DATE_RELEASE</Name>
            <String>XMLTAG_DATE_RELEASE</String>
        </Simple>
        <Simple>
            <Name>TOTAL_PARTS</Name>
            <String>XMLTAG_EPISODE_TOTAL</String> <!-- number of lower level 50=episodes -->
        </Simple>
    </Tag>
    <Tag> <!-- episode -->
        <Targets>
            <TargetTypeValue>50</TargetTypeValue>
        </Targets>
        <Simple>
            <Name>PART_NUMBER</Name>
            <String>XMLTAG_EPISODE_NUMBER</String>
        </Simple>
        <Simple>
            <Name>TITLE</Name>
            <String>XMLTAG_EPISODE_TITLE</String>
        </Simple>
        <Simple>
          <Name>DATE_ENCODED</Name>
          <String>XMLTAG_DATE_ENCODED</String>
        </Simple>
        <Simple>
          <Name>ENCODED_BY</Name>
          <String>XMLTAG_ENCODED_BY</String>
        </Simple>
        <Simple>
          <Name>COMMENT</Name>
          <String>XMLTAG_COMMENT</String>
        </Simple>
    </Tag>
</Tags>
